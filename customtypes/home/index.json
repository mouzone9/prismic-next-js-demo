{
  "id": "home",
  "label": "Home",
  "format": "page",
  "repeatable": false,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": { "label": "homeId", "placeholder": "" }
      },
      "domain": {
        "type": "StructuredText",
        "config": {
          "label": "Domain",
          "placeholder": "",
          "allowTargetBlank": true,
          "single": "paragraph"
        }
      },
      "test": {
        "type": "Text",
        "config": { "label": "Test", "placeholder": "" }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": { "choices": { "home_slice": { "type": "SharedSlice" } } }
      }
    },
    "SEO & Metadata": {
      "meta_description": {
        "config": {
          "label": "Meta Description",
          "placeholder": "A brief summary of the page"
        },
        "type": "Text"
      },
      "meta_image": {
        "config": {
          "constraint": { "height": 1260, "width": 2400 },
          "label": "Meta Image",
          "thumbnails": []
        },
        "type": "Image"
      },
      "meta_title": {
        "config": {
          "label": "Meta Title",
          "placeholder": "A title of the page used for social media and search engines"
        },
        "type": "Text"
      }
    }
  }
}
